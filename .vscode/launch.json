{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current JavaScript File",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "cwd": "${fileDirname}",
      "runtimeExecutable": "/opt/homebrew/bin/node",
      "outFiles": []
    }
  ]
}
